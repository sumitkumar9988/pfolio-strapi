    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "theme": {
      "type": "enumeration",
      "enum": [
        "dark",
        "light"
      ],
      "required": true,
      "default": "light"
    }
  }
